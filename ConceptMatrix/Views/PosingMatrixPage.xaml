                            <TabItem Header="Body" Style="{DynamicResource MenuLevel2}">
                                <StackPanel>
                                    <GroupBox Header=" Body" Padding="0,0,0,5">
                                        <StackPanel>
                                            <StackPanel Orientation="Vertical">
                                                <StackPanel Orientation="Horizontal">
                                                    <Label Content="Neck:" Foreground="#737779" VerticalAlignment="Center" Width="80"/>
                                                    <ToggleButton Unchecked="Toggles_Unchecked" IsEnabled="False" Checked="Toggles_Checked" Name="Neck"/>
