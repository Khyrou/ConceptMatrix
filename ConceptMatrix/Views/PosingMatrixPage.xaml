                                                    <Label Content="Earrings:" Foreground="#737779" VerticalAlignment="Center" Width="65"/>
                                                    <ToggleButton Unchecked="Toggles_Unchecked" IsEnabled="False" Name="EarringALeft" Checked="Toggles_Checked" Content="L1"/>
                                                    <ToggleButton Unchecked="Toggles_Unchecked" IsEnabled="False" Name="EarringARight" Checked="Toggles_Checked" Content="R1"/>
                                                </StackPanel>
                                                <StackPanel Orientation="Horizontal">
                                                    <Label VerticalAlignment="Center" Width="80"/>
                                                    <ToggleButton Unchecked="Toggles_Unchecked" IsEnabled="False" Checked="Toggles_Checked" Name="ExHairG" Content="7"/>
                                                    <ToggleButton Unchecked="Toggles_Unchecked" IsEnabled="False" Checked="Toggles_Checked" Name="ExHairH" Content="8"/>
                                                    <ToggleButton Unchecked="Toggles_Unchecked" IsEnabled="False" Checked="Toggles_Checked" Name="ExHairI" Content="9"/>
                                                    <ToggleButton Unchecked="Toggles_Unchecked" IsEnabled="False" Checked="Toggles_Checked" Name="ExHairJ" Content="10"/>
                                                    <ToggleButton Unchecked="Toggles_Unchecked" IsEnabled="False" Checked="Toggles_Checked" Name="ExHairK" Content="11"/>
                                                    <ToggleButton Unchecked="Toggles_Unchecked" IsEnabled="False" Checked="Toggles_Checked" Name="ExHairL" Content="12"/>
