                                                <TextBlock Margin="2 -1 0 0" VerticalAlignment="Center" Padding="0" Text="1" FontSize="16" />
                                            </StackPanel>
                                        </Button>
                                        <Button x:Name="LoadstateHead01" materialDesign:ShadowAssist.ShadowDepth="Depth3" HorizontalAlignment="Left"  Margin="10,0,0,0" VerticalAlignment="Top" Width="50" Height="28" Padding="0" IsEnabled="False" Click="LoadstateHead01_Click">
                                            <StackPanel Orientation="Horizontal" Width="50" Height="16">
