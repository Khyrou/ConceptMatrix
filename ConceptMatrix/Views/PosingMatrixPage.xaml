                                                    <ToggleButton Unchecked="Toggles_Unchecked" IsEnabled="False" Checked="Toggles_Checked"  Name="HrothWhiskersLeft" Content="L"/>
                                                    <ToggleButton Unchecked="Toggles_Unchecked" IsEnabled="False" Checked="Toggles_Checked" Name="HrothWhiskersRight" Content="R"/>
                                                </StackPanel>
                                                <StackPanel Orientation="Horizontal">
                                                    <Label VerticalAlignment="Center" Width="80"/>
                                                    <ToggleButton Unchecked="Toggles_Unchecked" IsEnabled="False" Name="HairA" Checked="Toggles_Checked" Content="1"/>
                                                    <ToggleButton Unchecked="Toggles_Unchecked" IsEnabled="False" Name="HairB" Checked="Toggles_Checked" Content="2"/>

