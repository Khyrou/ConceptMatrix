                                                <TextBlock Margin="2 -1 0 0" VerticalAlignment="Center" Padding="0" Text="2" FontSize="16" />
                                            </StackPanel>
                                        </Button>
                                    </StackPanel>
                                    <GroupBox Header=" Hair and Accessories" Margin="0,5,0,0" Padding="0,0,0,5">
                                        <StackPanel>
                                            <StackPanel Orientation="Vertical">
                                                <StackPanel Orientation="Horizontal">
                                                    <Label Content="General:" Foreground="#737779" VerticalAlignment="Center" Width="80"/>
