<UserControl x:Class="ConceptMatrix.Views.PosingMatrixPage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ConceptMatrix.Views"
             xmlns:core="clr-namespace:System;assembly=mscorlib"
             xmlns:mah="http://metro.mahapps.com/winfx/xaml/controls"
             xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
             mc:Ignorable="d" 
             d:DesignHeight="1822" d:DesignWidth="444">
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="../Controls/TextBoxStyle.xaml"/>
            </ResourceDictionary.MergedDictionaries>
            <core:Double x:Key="ColumnWidth1">134</core:Double>
            <core:Double x:Key="ColumnWidth2">200</core:Double>
            <Style x:Key="MenuLevel2" BasedOn="{StaticResource MetroTabItem}" TargetType="{x:Type TabItem}">
                <Setter Property="mah:ControlsHelper.HeaderFontSize" Value="16"></Setter>
            </Style>
            <Style TargetType="TabControl" x:Key="MyTabControl">
                <Setter Property="Template">
                    <Setter.Value>
                        <ControlTemplate TargetType="TabControl">
                            <Grid SnapsToDevicePixels="true" KeyboardNavigation.TabNavigation="Local" HorizontalAlignment="Stretch">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="*" />
                                </Grid.RowDefinitions>
                                <WrapPanel x:Name="HeaderPanel" IsItemsHost="true" Grid.Row="0" KeyboardNavigation.TabIndex="1" />
                                <ContentPresenter Grid.Row="1" x:Name="PART_SelectedContentHost" ContentSource="SelectedContent" Margin="0,10,0,0"/>
                            </Grid>
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
            </Style>
        </ResourceDictionary>
    </UserControl.Resources>
    <Grid>
        <DockPanel>
            <ScrollViewer DockPanel.Dock="Bottom" Margin="0,0,-13,0" Padding="5,0,0,0">
                <Grid>
                    <StackPanel>
                        <GroupBox Header="Posing Matrix Settings (Only in Gpose)" Name="PoseMatrixSetting" Width="Auto" Height="110" Padding="9,0,9,2" IsEnabled="False" >
                            <StackPanel>
                                <StackPanel Orientation="Horizontal">
                                    <Label Content="Edit Mode:" Foreground="#737779" VerticalAlignment="Center" Width="87"/>
                                    <ToggleButton x:Name="EditModeButton" Checked="EditModeButton_Checked" Unchecked="EditModeButton_Unchecked"></ToggleButton>
                                    <Label Content="Parenting Mode:" Foreground="#737779"  VerticalAlignment="Center"/>
                                    <ToggleButton IsChecked="{Binding ParentingToggle}"/>
                                    <Label Content="Helm Parenting Mode:" Foreground="#737779"  VerticalAlignment="Center" Width="130"/>
                                    <ToggleButton x:Name="HelmToggle" IsEnabled="False" Checked="HelmToggle_Checked" Unchecked="HelmToggle_Unchecked" />
                                </StackPanel>
                                <StackPanel Orientation="Horizontal">
                                    <Label Content="Enable Physics:" Foreground="#737779"  VerticalAlignment="Center"/>
                                    <ToggleButton Name="PhysicsButton" Checked="PhysicsButton_Checked" Unchecked="PhysicsButton_Unchecked" IsEnabled="False" />
                                    <Label Content="Scaling Mode:" Foreground="#737779" VerticalAlignment="Center" Width="95"/>
                                    <ToggleButton x:Name="ScaleToggle" IsEnabled="False" Checked="ScaleToggle_Checked" Unchecked="ScaleToggle_Unchecked" />
                                    <Label Content="Weapon Postion Mode:" Foreground="#737779" VerticalAlignment="Center"/>
                                    <ToggleButton x:Name="WeaponPoSToggle" IsEnabled="False" Checked="WeaponPoSToggle_Checked" Unchecked="WeaponPoSToggle_Unchecked"/>
                                </StackPanel>
                                <StackPanel Orientation="Horizontal">
                                    <Button x:Name="SaveCMP" Width="156" Margin="25,5,0,0" Click="SaveCMP_Click">
                                        <StackPanel Orientation="Horizontal">
                                            <materialDesign:PackIcon Kind="ContentSave" Padding="0" Margin="0,2,0,0" />
                                            <TextBlock Margin="8 0 0 0" VerticalAlignment="Center" Padding="0" Text="Save CMP FIle" />
                                        </StackPanel>
                                    </Button>
                                    <Button x:Name="LoadCMP" Width="156" Margin="50,5,0,0" Click="LoadCMP_Click">
                                        <StackPanel Orientation="Horizontal">
                                            <materialDesign:PackIcon Kind="Download" Padding="0" Margin="0,2,0,0" />
                                            <TextBlock Margin="8 0 0 0" VerticalAlignment="Center" Padding="0" Text="Load CMP FIle" />
                                        </StackPanel>
                                    </Button>
                                </StackPanel>
                            </StackPanel>
                        </GroupBox>

                        <StackPanel Orientation="Horizontal">
                            <Label Content="X Rotation:" Foreground="#737779"  Width="{StaticResource ColumnWidth1}" VerticalAlignment="Center"/>
                            <mah:NumericUpDown x:Name="XUpDown" Width="125" Value="{Binding UserBoneX, Mode=TwoWay, NotifyOnSourceUpdated=True}" Style="{DynamicResource MaterialDesignNumericUpDown}" Background="{x:Null}" TextAlignment="Left"  BorderThickness="2" Maximum="360" Minimum="0" StringFormat="F6" Interval="0.01"/>
                            <Slider x:Name="BoneSliderX" Value="{Binding UserBoneX, Mode=TwoWay, NotifyOnSourceUpdated=True}" HorizontalAlignment="Left"  VerticalAlignment="Top"
                                Margin="5,5,0,0" Width="165" Minimum="0" Maximum="360" TickFrequency="0.5" />
                        </StackPanel>
                        <StackPanel Orientation="Horizontal">
                            <Label Content="Y Rotation:" Foreground="#737779"  Width="{StaticResource ColumnWidth1}" VerticalAlignment="Center"/>
                            <mah:NumericUpDown x:Name="YUpDown" Width="125" Value="{Binding UserBoneY, Mode=TwoWay, NotifyOnSourceUpdated=True, UpdateSourceTrigger=PropertyChanged}" Style="{DynamicResource MaterialDesignNumericUpDown}" Background="{x:Null}" TextAlignment="Left"  BorderThickness="2" Maximum="360" Minimum="0" StringFormat="F6" Interval="0.01"/>
                            <Slider x:Name="BoneSliderY" Value="{Binding UserBoneY, Mode=TwoWay, NotifyOnSourceUpdated=True, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left"  VerticalAlignment="Top"
                                Margin="5,5,0,0" Width="165" Minimum="0" Maximum="360" TickFrequency="0.5" />
                        </StackPanel>
                        <StackPanel Orientation="Horizontal">
                            <Label Content="Z Rotation:" Foreground="#737779"  Width="{StaticResource ColumnWidth1}" VerticalAlignment="Center"/>
                            <mah:NumericUpDown x:Name="ZUpDown" Width="125" Value="{Binding UserBoneZ, Mode=TwoWay, NotifyOnSourceUpdated=True, UpdateSourceTrigger=PropertyChanged}" Style="{DynamicResource MaterialDesignNumericUpDown}" Background="{x:Null}" TextAlignment="Left"  BorderThickness="2" Maximum="360" Minimum="0" StringFormat="F6" Interval="0.01"/>
                            <Slider x:Name="BoneSliderZ" Value="{Binding UserBoneZ, Mode=TwoWay, NotifyOnSourceUpdated=True, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left"  VerticalAlignment="Top"
                                Margin="5,5,0,0" Width="165" Minimum="0" Maximum="360" TickFrequency="0.5"/>
                        </StackPanel>
                        <mah:MetroAnimatedTabControl Background="{x:Null}" Style="{DynamicResource MyTabControl}">
